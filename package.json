    "log4js": "^6.7.1",
    "moment": "^2.29.4",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.0",
    "pug": "^3.0.2",
    "randomatic": "^3.1.1",
    "uuid": "^9.0.0",
    "vitest": "^0.28.3"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "^0.28.3",
